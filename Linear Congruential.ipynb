    "# This code implements the linear congruential method for generating random integers.\n",
    "\n",
    "import numpy as np\n",
    "\n",
    "nums = 20\n",
    "a = 7\n",
    "b = 3\n",
    "c = 16\n",
    "x = 270  # This will be the seed.\n",
    "\n",
    "for i in range(nums):\n",
    "    x = (a * x + b) % c\n",
    "    print(x, \"\", end='')"
